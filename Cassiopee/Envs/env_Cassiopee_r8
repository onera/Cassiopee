if ($?CASSIOPEE == 0) then
    echo "You must specify a CASSIOPEE variable in your environment."
    echo "This variable specifies the installation path of *Cassiopee*."
    exit
endif
# source env from bash env
if (-e $CASSIOPEE/Cassiopee/Envs/env_Cassiopee_local) then
   bash $CASSIOPEE/Cassiopee/Envs/env_Cassiopee_local
else
   bash $CASSIOPEE/Dist/env_Cassiopee_local
endif
chmod +x /tmp/.vars.csh
source /tmp/.vars.csh
rm -f /tmp/.vars.csh
if ("$PYTHONEXE" == "python3") then
   alias python python3
endif
#cat .vars.csh
